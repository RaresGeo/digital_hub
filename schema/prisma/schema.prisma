generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    String  @id @db.Uuid
  email String  @unique
  name  String?

  registeredAt DateTime  @default(now())
  lastLoggedIn DateTime?

  @@map("users")
}
